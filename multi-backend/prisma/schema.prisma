generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id_user Int     @id @default(autoincrement())
  name    String? @db.VarChar(200)
  email    String? @db.VarChar(200)
  password    Bytes? @db.Blob
  salt        String? @db.VarChar(45)
}
